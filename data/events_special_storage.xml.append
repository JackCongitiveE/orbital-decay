<mod:findName type="event" name="COMBAT_CHECK_TOGGLE"> <!-- Finding Toggle Menu event -->
	<mod-append:choice req="CO_BALLISTATOG_DEFEND" blue="false"> <!-- Appending new choice option to event -->
		 <text>(Ballista Drone) Set the state of the drone. [Current mode: Anti-Person]</text>
		 <event load="COMBAT_CHECK_CO_BALLISTATOG_DEFEND"/>
	</mod-append:choice>
	<mod-append:choice req="CO_BALLISTATOG_BOARDER" blue="false"> <!-- Appending new choice option to event -->
		 <text>(Ballista Drone) Set the state of the drone. [Current mode: Boarder]</text>
		 <event load="COMBAT_CHECK_CO_BALLISTATOG_BOARDER"/>
	</mod-append:choice>
	
	<mod-append:choice req="CO_COMMITTOG_DEFEND" blue="false"> <!-- Appending new choice option to event -->
		 <text>(Commit Drone) Set the state of the drone. [Current mode: Anti-Person]</text>
		 <event load="COMBAT_CHECK_CO_COMMITTOG_DEFEND"/>
	</mod-append:choice>
	<mod-append:choice req="CO_COMMITTOG_BOARDER" blue="false"> <!-- Appending new choice option to event -->
		 <text>(Commit Drone) Set the state of the drone. [Current mode: Boarder]</text>
		 <event load="COMBAT_CHECK_CO_COMMITTOG_BOARDER"/>
	</mod-append:choice>
