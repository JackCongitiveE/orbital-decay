<!--
	<mod:findWithChildLike type="choice" child-type="text">
		<mod:selector>Install the onboard lab.</mod:selector>
		<mod-overwrite:text>Install the onboard lab. [TESTTTTT]</mod-overwrite:text>
	</mod:findLike>
	
	CO_CHECK_NO_SIGIL_HEALING_MR_AND_NO_LAB_INSTALLED
	
	CO_CHECK_NO_SIGIL_HEALING_MR_AND_LAB_INSTALLED
	
	
	-->
<![CDATA[

<mod:findName type="event" name="STORAGE_CHECK_BUTTON">
	<!-- MR HEALING SIGIL EFFECT-->
	<mod:findLike type="choice">
		<mod:selector req="LAB_INSTALLED" lvl="0" hidden="true" blue="false" />
		<mod:setAttributes req="CO_CHECK_NO_SIGIL_HEALING_MR_AND_NO_LAB_INSTALLED" hidden="true" blue="false" />
		<mod-overwrite:text>Install the onboard lab. [TESTTTTT]</mod-overwrite:text>
	</mod:findLike>
	<mod:findLike type="choice">
		<mod:selector req="LAB_INSTALLED" lvl="1" hidden="true" blue="false" />
		<mod:setAttributes req="CO_CHECK_NO_SIGIL_HEALING_MR_AND_LAB_INSTALLED" lvl="1" hidden="true" blue="false" />
		<mod-overwrite:text>Check the onboard lab. [TESTTTTT]</mod-overwrite:text>
	</mod:findLike>
	<mod-append:choice req="CO_SIGIL_HEALING_MR" hidden="true" blue="false">	
		<text color="co_ravager_red">Something doesn't allow you to use the onboard lab.</text>
		<event load="OPTION_INVALID"/>
	</mod-append:choice>
</mod:findName>

<mod:findName type="event" name="STORAGE_CHECK_SYSTEM">
	<!-- HR HEALING SIGIL EFFECT-->
	<mod:findLike type="choice">
		<mod:selector req="medbay" hidden="true" blue="false" />
		<mod:setAttributes req="CO_CHECK_NO_SIGIL_HEALING_HR_AND_MEDBAY" hidden="true" blue="false" />
		<mod-overwrite:text>Medbay. [TESTTTTT]</mod-overwrite:text>
	</mod:findLike>
	<mod-append:choice req="CO_CHECK_SIGIL_HEALING_HR_AND_MEDBAY" hidden="true" blue="false">	
		<text color="co_ravager_red">Something doesn't allow you to upgrade the Medbay.</text>
		<event load="OPTION_INVALID"/>
	</mod-append:choice>
	<mod:findLike type="choice">
		<mod:selector req="clonebay" hidden="true" blue="false" />
		<mod:setAttributes req="CO_CHECK_NO_SIGIL_HEALING_HR_AND_CLONEBAY" hidden="true" blue="false" />
		<mod-overwrite:text>Clonebay. [TESTTTTT]</mod-overwrite:text>
	</mod:findLike>
	<mod-append:choice req="CO_CHECK_SIGIL_HEALING_HR_AND_CLONEBAY" hidden="true" blue="false">	
		<text color="co_ravager_red">Something doesn't allow you to upgrade the Clonebay.</text>
		<event load="OPTION_INVALID"/>
	</mod-append:choice>
</mod:findName>




<!--////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
														SIGIL MENU
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
<mod:findName type="event" name="STORAGE_CHECK_BUTTON">
	<mod-append:choice blue="false">	
		<text>Check active sigils.</text>
		<event load="CO_LOAD_ACTIVE_SIGIL_CHECK"/>
	</mod-append:choice>
</mod:findName>
]]>

<event name="CO_LOAD_ACTIVE_SIGIL_CHECK">
	<loadEvent>CO_EVENT_ACTIVE_SIGIL_CHECK</loadEvent>
</event>

<event name="CO_EVENT_ACTIVE_SIGIL_CHECK">
	<text>In this menu you can get information about the Sigils that are currently active. Only the one who put these Sigils on you can destroy them.</text>
	<choice hidden="true">
		<text>Low Risk Sigils.</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice hidden="true">
		<text>Medium Risk Sigils.</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice hidden="true">
		<text>High Risk Sigils.</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice hidden="true">
		<text color="co_black">Curses.</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice hidden="true">
		<text>Do something else.</text>
		<event load="STORAGE_CHECK"/>
	</choice>
	
	<choice req="PLAYER_SHIP_FLAGSHIP_UNLOCKED" lvl="1" hidden="true">
		<text>Flagship</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="PLAYER_SHIP_FLAGSHIP_UNLOCKED" lvl="0" max_lvl="0" hidden="true">
		<text>No Flagship</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="PLAYER_SHIP_FLAGSHIP_2_UNLOCKED" lvl="1" hidden="true">
		<text>Flagship 2</text>
		<event load="OPTION_INVALID"/>
	</choice>
	<choice req="PLAYER_SHIP_FLAGSHIP_2_UNLOCKED" lvl="0" max_lvl="0" hidden="true">
		<text>No Flagsip 2</text>
		<event load="OPTION_INVALID"/>
	</choice>
</event>









